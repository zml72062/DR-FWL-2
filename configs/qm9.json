
{
    "dataset": {
        "root": "datasets/qm9",
        "target": 0
    },
    "preprocess": {
        "convert": "post",
        "not_normalize_dist": false,
        "use_relative_pos": false,
        "squared_dist": false
    },
    "model": {
        "hidden_channels": 64,
        "num_layers": 5,
        "dropout": 0.0,
        "norm": "layer_norm",
        "in_layer_norm": "layer_norm",
        "eps": 0.0,
        "train_eps": false,
        "add_0": true,
        "add_112": true,
        "add_212": true,
        "add_222": true,
        "residual": "last"
    },
    "train": {
        "seed": 42,
        "batch_size": 512,
        "cuda": 0,
        "epochs": 400,
        "lr": 0.001000,
        "adam_beta1": 0.9,
        "adam_beta2": 0.999,
        "adam_eps": 1e-8,
        "l2_penalty": 0.0,
        "lr_reduce_factor": 0.9,
        "lr_reduce_patience": 10,
        "lr_reduce_min": 1e-5
    }
}
        

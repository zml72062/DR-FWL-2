{
    "dataset": {
        "root": "datasets/zinc",
        "target": "y"
    },
    "model": {
        "in_channels": 80,
        "hidden_channels": 80,
        "num_layers": 6,
        "dropout": 0.0,
        "norm": "batch_norm",
        "mlp_num_layers": 1,
        "eps": 0.0,
        "eps2": 0.0,
        "train_eps": true,
        "post_mlp_num_layers": 2
    },
    "train": {
        "seed": 42,
        "batch_size": 64,
        "cuda": -1,
        "epochs": 500,
        "lr": 0.001,
        "adam_beta1": 0.9,
        "adam_beta2": 0.999,
        "adam_eps": 1e-8,
        "l2_penalty": 0.0,
        "lr_reduce_factor": 0.5,
        "lr_reduce_patience": 20,
        "lr_reduce_min": 1e-6,
        "num_workers": 4,
        "test_eval_interval": 10
    }
}
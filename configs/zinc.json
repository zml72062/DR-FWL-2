{
    "dataset": {
        "root": "datasets/zinc",
        "target": "y"
    },
    "model": {
        "hidden_channels": 80,
        "num_layers": 6,
        "dropout": 0.0,
        "norm": "batch_norm",
        "eps": 0.0,
        "add_112": true,
        "add_212": true,
        "add_222": true,
        "add_vv": true,
        "residual": "last"
    },
    "train": {
        "seed": 42,
        "batch_size": 128,
        "cuda": -1,
        "epochs": 500,
        "lr": 0.001,
        "adam_beta1": 0.9,
        "adam_beta2": 0.999,
        "adam_eps": 1e-8,
        "l2_penalty": 0.0,
        "lr_reduce_factor": 0.5,
        "lr_reduce_patience": 20,
        "lr_reduce_min": 1e-6,
        "num_workers": 0,
        "test_eval_interval": 10
    }
}
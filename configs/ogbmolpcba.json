{
    "dataset": {
        "name": "ogbg-molpcba",
        "root": "datasets"
    },
    "model": {
        "hidden_channels": 256,
        "num_layers": 3,
        "dropout": 0.2,
        "norm": "layer_norm",
        "in_layer_norm": "layer_norm",
        "eps": 0.0,
        "train_eps": false,
        "add_0": true,
        "add_112": true,
        "add_212": true,
        "add_222": true,
        "residual": "last"
    },
    "train": {
        "seed": 42,
        "batch_size": 256,
        "cuda": 1,
        "epochs": 150,
        "lr": 0.0005,
        "adam_beta1": 0.9,
        "adam_beta2": 0.999,
        "adam_eps": 1e-8,
        "l2_penalty": 0.0,
        "lr_reduce_factor": 0.8,
        "lr_step_size": 10
    }
}